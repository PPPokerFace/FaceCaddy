http://192.168.1.102
{
	redir https://{host}{uri}
}

http://127.0.0.1:8000
{
	proxy / http://192.168.1.102:8000
}

https://192.168.1.102
{
	gzip {
		level 9
	}
	proxy /b http://127.0.0.1:8000 {
		without /b
		websocket
		transparent
		except static
	}
	proxy / http://127.0.0.1:8080
	tls self_signed
}
